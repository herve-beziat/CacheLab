    "start": "node dist/server.js",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/herve-beziat/CacheLab.git"
  },
  "keywords": [
    "typescript",
    "nodejs",
    "cache",
    "redis",
    "key-value-store",
    "education"
  ],
  "author": "Hervé BEZIAT",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/herve-beziat/CacheLab/issues"
  },
  "homepage": "https://github.com/herve-beziat/CacheLab#readme",
  "devDependencies": {
    "@types/node": "^24.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "rimraf": "^5.0.5"
  }
}
